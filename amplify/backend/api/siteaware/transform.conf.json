    "Version": 5,
    "ElasticsearchWarning": true,
    "ResolverConfig": {
        "project": {
            "ConflictHandler": "AUTOMERGE",
            "ConflictDetection": "VERSION"
        }
    },
    "TableNameOverrides": [
        {
            "ModelName": "Employee",
            "TableName": "Employee-${env}"
        },
        {
            "ModelName": "Client",
            "TableName": "Client-${env}"
        }
    ]
}